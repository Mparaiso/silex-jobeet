{# /src/Mparaiso/JobBoard/Resources/views/mp.jobb.job.latest.atom.twig #}
{#{% spaceless %}#}
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>{{ app['mp.jobb.params.title'] }}</title>
    <subtitle>Latest Jobs</subtitle>
    <link href="{{ url('job_latest',{_format: "atom"}) }}" rel="self"/>
    <link href="{{ url('job_latest',{_format: "atom"}) }}"/>
    <updated>{{ app['mp.jobb.service.job'].getLatestJob().createdAt |date('c') }}</updated>
    <author>
        <name>Jobeet</name>
    </author>
    <id>{{ url('job_latest',{_format: "atom"}) }}</id>
    {% for job in jobs %}
        {% include 'mp.jobb.job.atom.entry.xml.twig' with {job:job} %}
    {% endfor %}
</feed>
{#{% endspaceless %}#}